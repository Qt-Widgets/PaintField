{
	".key-bindings":
	{
		"paintfield.file.new": "Ctrl+N",
		"paintfield.file.open": "Ctrl+O",
		"paintfield.file.save": "Ctrl+S",
		"paintfield.file.saveAs": "Ctrl+Shift+S",
		"paintfield.file.close": "Ctrl+W",
		"paintfield.file.quit": "Ctrl+Q",

		"paintfield.edit.undo": "Ctrl+Z",
		"paintfield.edit.redo": "Ctrl+Shift+Z",
		"paintfield.edit.cut": "Ctrl+X",
		"paintfield.edit.copy": "Ctrl+C",
		"paintfield.edit.paste": "Ctrl+V",
		"paintfield.edit.selectAll": "Ctrl+A",

		"paintfield.layer.newRaster": "Ctrl+L",

		"paintfield.window.minimize": "Ctrl+M",
		"paintfield.window.newWorkspace": "Ctrl+Shift+N",
		"paintfield.window.closeWorkspace": "Ctrl+Shift+W",
		
		"paintfield.canvas.dragTranslation": "Space",
		"paintfield.canvas.dragScale": "Ctrl+Space",
		"paintfield.canvas.dragRotation": "Shift+Space"
	}
}
